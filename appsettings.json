{
  "AllowedHosts": "*",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "None", // Debug
      "System": "None",
      "Microsoft": "None",
      "Microsoft.AspNetCore.SignalR": "None",
      "Gtpx.Cloud.Api.Web.Controllers.PackageController": "Trace",
      "Gtpx.Cloud.Services.Reports": "Trace"
    },    
    "CallStack": {
      "LogLevel": {
        "Default": "Error", // Debug
        "System": "Error",
        "Microsoft": "Error",
        "Microsoft.AspNetCore.SignalR": "Error",
        "Gtpx.Cloud.Api.Web.Controllers.PackageController": "Trace",
        "Gtpx.Cloud.Services.Reports": "Trace"
      }
    }
  },
  "Forge": {
      "A360RefreshTimeSpan": "01:12:00:00"
  },
  "AuthenticatedSessionCacheService": {
      "ExpirationTimeSpan": "13:23:00:00"
  }
}
